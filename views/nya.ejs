<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
  <title></title>
</head>

<body>
  <h1 style="text-align:center">उपलब्ध वाहन और उनके मालिक</h1>

  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th>दूरी</th>
          <th>मोबाइल नंबर</th>
          <th>क्षमता</th>
        </tr>

        <% vehicleDetails.forEach(function(vehicle){ %>
          <tr>
            <td><%=vehicle.dist%></td>
            <td><%=vehicle.location.phone%></td>
            <td><%=vehicle.location.capacity%> किलो</td>
            <td><a href="#"></a></td>
          </tr>
        <% }) %>
      </thead>
      <%  if(vehicleDetails.length==0){%>
          <h1>no data available</h1>
        <%}%>
    </table>
  </div>
</body>

</html>
